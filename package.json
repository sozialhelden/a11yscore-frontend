{
  "name": "a11yscore-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "echo 'no unit tests yet'",
    "test:e2e": "echo 'no e2e tests yet'",
    "transifex:sync": "env-cmd --use-shell -- txjs-cli push app/ --token=\\$TRANSIFEX_TOKEN --secret=\\$TRANSIFEX_SECRET",
    "postinstall": "husky"
  },
  "dependencies": {
    "@react-router/node": "7.9.5",
    "@react-router/serve": "7.9.5",
    "@sozialhelden/core": "2.3.0",
    "@sozialhelden/ui": "1.8.2",
    "@transifex/native": "7.1.4",
    "@transifex/react": "7.1.4",
    "dompurify": "3.3.0",
    "isbot": "5.1.32",
    "lucide-react": "0.553.0",
    "luxon": "3.7.2",
    "marked": "16.4.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@react-router/dev": "7.9.5",
    "@tailwindcss/vite": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@transifex/cli": "7.1.4",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "env-cmd": "11.0.0",
    "husky": "9.1.7",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-tsconfig-paths": "5.1.4"
  }
}
